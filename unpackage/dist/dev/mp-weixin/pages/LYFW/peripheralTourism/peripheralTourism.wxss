/* 搜索栏区域样式 */
.searchArea {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  padding-left: 10rpx;
  text-align: center;
}
/* 左上角所在城市文本 */
.locationTxt {
  color: #333333;
  padding-top: 8px;
  padding-left: 8px;
  width: 70px;
  font-size: 36rpx;
  font-weight: bold;
}
/* 景点搜索框 */
.inputIocale {
  text-align: left;
  background-color: #F5F5F5;
  width: 166px;
  background-image: url(data:image/png;base64,);
  background-size: 48rpx 50rpx;
  background-position: 28rpx 23rpx;
  /*小图标在input的位置*/
  background-repeat: no-repeat;
  padding: 6px 6px 6px 40px;
  /*设置input内边距*/
  margin-left: 5px;
  border-radius: 46rpx;
  font-size: 29rpx;
}
/* 向下小图标 */
.icon {
  font-size: 20rpx;
  color: #AAAAAA;
  margin-left: 15rpx;
}
.box {
  position: -webkit-sticky;
  position: sticky;
  top: 0;
  background: #fff;
}
.Tk_scrollview {
  width: 100%;
  padding: 16rpx 32rpx;
  margin: 0 auto;
}
.Tk_scrollview .Tk_item {
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
}
.Tk_scrollview .Tk_item .Tk_image {
      width: 182rpx;
      height: 152rpx;
      border-radius: 12rpx;
      margin: 24rpx 0rpx;
}
.Tk_scrollview .Tk_item .Tk_bacg {
      margin-top: 20rpx;
      margin-left: 24rpx;
}
.Tk_scrollview .Tk_item .Tk_text1 {
      display: -webkit-box;
      display: -webkit-flex;
      display: flex;
      text-overflow: ellipsis;
      white-space: nowrap;
      overflow: hidden;
      width: 480rpx;
}
.Tk_scrollview .Tk_item .Tk_text2 {
      font-size: 26rpx;
      margin-top: 20rpx;
      display: block;
}
.Tk_scrollview .Tk_item .Tk_text3 {
      font-size: 24rpx;
      margin-top: 10rpx;
      color: #AAAAAA;
      display: block;
}
.Tk_scrollview .Tk_btn {
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    width: 100%;
    font-size: 28rpx;
    color: #AAAAAA;
    padding-right: 32rpx;
}
.Tk_scrollview .Tk_btn .right {
      position: relative;
      margin-left: 12rpx;
      left: 368rpx;
      bottom: 6rpx;
}
.Tk_scrollview .Tk_btn .yticon {
      position: relative;
      font-size: 28rpx;
      margin-left: 12rpx;
      left: 368rpx;
}
.currencyTitle {
  font-size: 36rpx;
  font-weight: bold;
  color: #333333;
  padding-left: 32rpx;
  margin-top: 24rpx;
  margin-bottom: 8rpx;
}
.sixBackground {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
  -webkit-box-orient: horizontal;
  -webkit-box-direction: normal;
  -webkit-flex-direction: row;
          flex-direction: row;
  -webkit-flex-wrap: wrap;
          flex-wrap: wrap;
  padding: 20rpx;
}
.sixBackground .darkCurtain {
    position: absolute;
    overflow: hidden;
    width: 212rpx;
    height: 144rpx;
    border-radius: 12rpx;
    margin: 12rpx 12rpx;
    background: rgba(7, 17, 27, 0.2);
}
.sixBackground image {
    width: 212rpx;
    height: 144rpx;
    border-radius: 12rpx;
    opacity: 1.5;
    margin: 12rpx 12rpx;
}
.sixBackground .sixView {
    position: absolute;
    text-overflow: ellipsis;
    white-space: nowrap;
    overflow: hidden;
    color: #fff;
    max-width: 192rpx;
    margin-left: 24rpx;
    margin-top: -106rpx;
}
.sixBackground .sixView .sixText1 {
      font-weight: bold;
      font-size: 30rpx;
}
.sixBackground .sixView .sixText2 {
      display: block;
      font-size: 24rpx;
      font-weight: 150;
}

